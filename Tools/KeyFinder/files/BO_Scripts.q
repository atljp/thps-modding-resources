script BO_Blader_Trick_Land 
    Obj_ClearExceptions
    ClearExceptionGroup BO_Blader_Trick_Dummy
    printf 'BO_Blader_Trick_Land'
    if GameModeEquals is_career
        if GetFlag flag = FLAG_BO_GOT_TRICK
            GetCurrentSkaterProfileIndex
            GetSkaterProfileInfo player = <currentSkaterProfileIndex>
            <Blader_Flips> = 0
            if NOT GoalManager_GoalIsLocked name = BO_Goal_Gaps11
                if (<skater_family> = family_pro)
                    printf 'Check it'
                    if NOT GoalManager_GoalFlagSet name = BO_Goal_Gaps11 flag = got_1 skater = pro
                        GoalManager_SetGoalFlag name = BO_Goal_Gaps11 got_1 1 skater = pro
                        printf 'Set It'
                    endif
                else
                    if (<name> = custom)
                        if NOT GoalManager_GoalFlagSet name = BO_Goal_Gaps11 flag = got_1 skater = <name>
                            GoalManager_SetGoalFlag name = BO_Goal_Gaps11 got_1 1 skater = <name>
                        endif
                    else
                        if (<name> = benfranklin)
                            if NOT GoalManager_GoalFlagSet name = BO_Goal_Gaps11 flag = got_1 skater = <name>
                                GoalManager_SetGoalFlag name = BO_Goal_Gaps11 got_1 1 skater = <name>
                            endif
                        endif
                    endif
                endif
                if GoalManager_GoalFlagSet name = BO_Goal_Gaps11 flag = got_1 skater = pro
                    <Blader_Flips> = (<Blader_Flips> + 1)
                endif
                if GoalManager_GoalFlagSet name = BO_Goal_Gaps11 flag = got_1 skater = custom
                    <Blader_Flips> = (<Blader_Flips> + 1)
                endif
                if GoalManager_GoalFlagSet name = BO_Goal_Gaps11 flag = got_1 skater = benfranklin
                    <Blader_Flips> = (<Blader_Flips> + 1)
                endif
                switch <Blader_Flips>
                    case 0
                    printf '0'
                    case 1
                    create_panel_message {text = '1 of 3 Blader Kickflips' style = goal_collect_text}
                    case 2
                    create_panel_message {text = '2 of 3 Blader Kickflips' style = goal_collect_text}
                    default 
                endswitch
            endif
            UnSetFlag flag = FLAG_BO_GOT_TRICK
        endif
    endif
    BO_Blader_Trick_Dummy
endscript


