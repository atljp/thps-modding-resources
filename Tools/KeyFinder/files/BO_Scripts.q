script BO_Spawn_SwitchSkaters 
    wait 2 frames
    Kill prefix = 'TRG_Ped_Gurney'
    pulse_blur
    swap_skaters_get_current_name
    switch <name>
        case custom
        BO_Segway_ParticlesOff
        if NOT GoalManager_HasWonGoal name = BO_Goal_Gaps7
            if NOT IsAlive name = TRG_Gurney_Ghost
                Create name = TRG_Gurney_Ghost
                printf 'Fuckup A'
            endif
        endif
        printf 'custom'
        BO_Unlock_You_Goals
        BO_SkateStopper_Hint_Check
        case benfranklin
        BO_Segway_ParticlesOff
        SetGlobalFlag flag = GUEST_FOUND_BENFRANKLIN
        SetGlobalFlag flag = SKATER_UNLOCKED_BENFRANKLIN
        BO_Unlock_Ben_Goals
        printf 'benfranklin'
        if NOT GoalManager_HasWonGoal name = BO_Goal_Gaps7
            if NOT IsAlive name = TRG_Gurney_Ghost
                Create name = TRG_Gurney_Ghost
                printf 'Fuckup B'
            endif
        endif
        BO_SkateStopper_Hint_Check
        case pro_teammate
        SetGlobalFlag flag = PRO_FOUND_01
        BO_Segway_ParticlesOff
        BO_Unlock_Pro_Goals
        printf 'muska'
        if NOT GoalManager_HasWonGoal name = BO_Goal_Gaps7
            if NOT IsAlive name = TRG_Gurney_Ghost
                Create name = TRG_Gurney_Ghost
                printf 'Fuckup C'
            endif
        endif
        BO_SkateStopper_Hint_Check
        case segwayrider
        SetGlobalFlag flag = GUEST_FOUND_JESSEJAMES
        BO_Unlock_Segway_Goals
        if NOT IsAlive name = TRG_BO_CarHopDummy
            Create name = TRG_BO_CarHopDummy
        endif
        if NOT GoalManager_HasWonGoal name = BO_Goal_SkateTheLine4
            if NOT IsAlive name = TRG_Race_Ped
                Create name = TRG_Race_Ped
                Create name = Paulie_Collision
            endif
        endif
        Kill name = TRG_Gurney_Ghost
        printf 'segwayrider'
        BO_SkateStopper2_Hint_Check
        default 
        BO_Segway_ParticlesOff
        printf 'default'
    endswitch
endscript


