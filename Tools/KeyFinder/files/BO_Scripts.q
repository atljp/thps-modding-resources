script BO_2d_sign_element_create parent = menu3d_anchor
    if GetGlobalFlag flag = SCREEN_MODE_WIDE
        <Offset> = (-8.0, 0.0)
    else
        <Offset> = (0.0, 0.0)
    endif
    CreateScreenElement {
        type = SpriteElement
        parent = <parent>
        texture = BO_icon_1
        id = BO_icon_smiley
        Scale = (1.5499999523162842, 1.5499999523162842)
        rgba = [ 128 128 128 50 ]
        Pos = ((62.0, 110.0) + <Offset>)
        rot_angle = 0
        just = [ left top ]
        z_priority = -51
    }
    CreateScreenElement {
        type = SpriteElement
        parent = <parent>
        texture = BO_icon_2
        id = BO_icon_deck
        Scale = (2.299999952316284, 1.399999976158142)
        rgba = [ 128 128 128 60 ]
        Pos = ((-23.0, 208.0) + <Offset>)
        rot_angle = 5
        just = [ left top ]
        z_priority = -51
    }
    RunScriptOnScreenElement id = BO_icon_smiley do_random_effect_paused params = { id = <id> }
    RunScriptOnScreenElement id = BO_icon_deck do_random_effect_paused params = { id = <id> }
endscript

