script BO_flame 
    if NOT InMultiPlayerGame
        if NOT GetFlag flag = FLAG_BO_SKATER_IS_ON_FIRE
            if NOT IsSkaterOnVehicle
                if GameModeEquals is_career
                    if NOT GoalManager_GoalIsLocked name = BO_Goal_HighCombo2
                        if NOT GoalManager_HasWonGoal name = BO_Goal_HighCombo2
                            GoalManager_GetDifficultyLevel
                            switch <difficulty_level>
                                case 0
                                create_panel_block {id = VertList
                                    text = 'Start and land a 5,000 point combo while on fire.'
                                    rgba = {70 , 70 , 144 , 10}
                                    Pos = (150.0, 250.0)
                                    time = 4500
                                    just = { left left }
                                    internal_just = { left left }
                                }
                                case 1
                                create_panel_block {id = VertList
                                    text = 'Start and land a 15,000 point combo while on fire.'
                                    rgba = {70 , 70 , 144 , 10}
                                    Pos = (150.0, 250.0)
                                    time = 4500
                                    just = { left left }
                                    internal_just = { left left }
                                }
                                case 2
                                create_panel_block {id = VertList
                                    text = 'Start and land a 50,000 point combo while on fire.'
                                    rgba = {70 , 70 , 144 , 10}
                                    Pos = (150.0, 250.0)
                                    time = 4500
                                    just = { left left }
                                    internal_just = { left left }
                                }
                            endswitch
                        endif
                    endif
                endif
                SetFlag flag = FLAG_BO_SKATER_IS_ON_FIRE
                goal_highcombo_enter_area goal_id = BO_Goal_HighCombo2
                SpawnSkaterScript BO_FireParticlesOff
                SpawnSkaterScript BO_SmokeParticlesOff
                wait 1 frame
                SpawnSkaterScript BO_FireParticlesOn
                SpawnSkaterScript BO_SmokeParticlesOn
                wait 5 seconds
                printf 'Wait 5 - 1'
                wait 5 seconds
                printf 'Wait 5 - 2'
                wait 5 seconds
                printf 'Wait 5 - 3'
                wait 5 seconds
                printf 'Wait 5 - 4'
                wait 5 seconds
                printf 'Wait 5 - 5'
                SpawnSkaterScript BO_FireParticlesOff
                UnSetFlag flag = FLAG_BO_SKATER_IS_ON_FIRE
                goal_highcombo_leave_area goal_id = BO_Goal_HighCombo2
                wait 3 seconds
                SpawnSkaterScript BO_SmokeParticlesOff
            endif
        endif
    endif
    UnSetFlag flag = FLAG_BO_SKATER_IS_ON_FIRE
endscript


