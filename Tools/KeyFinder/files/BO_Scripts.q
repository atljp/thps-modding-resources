script BO_Paramedic_Attack 
    ClearExceptionGroup BO_Paramedic
    printf 'Attack'
    obj_setinnerradius 5
    obj_setouterradius 50
    Obj_SetException Ex = objectinradius scr = BO_Paramedic_HitSkater group = BO_Paramedic
    Obj_SetException Ex = SkaterOutOfArea scr = BO_Paramedic_Return group = BO_Paramedic
    begin 
    Obj_LookAtObject type = skater time = 0.2500
    Obj_WaitRotate
    if NOT obj_getcollision height = 0.5000 Length = 15
        Obj_CycleAnim anim = Ped_M_RunFromIdle from = start to = 10
        obj_moveforward dist = 300 Speed = 10 acceleration = 1
    else
    endif
    Obj_CycleAnim anim = Ped_M_RunFromIdle from = 11 to = end
    Obj_PlayAnim anim = Ped_M_Run Cycle
    Obj_WaitMove
    Obj_CycleAnim anim = Ped_M_Idle1
