script BO_STATECONTROL_drummer_drumwalking 
    gettags
    obj_enableanimblending enabled = 1
    begin 
    Random (
        @ PlayAnimWithPartialAnimIfExists anim = Ped_Sldr_Marching01 BlendPeriod = 0
        change DrumPartialNumber = 1
        @ PlayAnimWithPartialAnimIfExists anim = Ped_Sldr_Marching01 PartialAnimOverlay = Ped_Sldr_Marching02_P
        change DrumPartialNumber = 2
        @ PlayAnimWithPartialAnimIfExists anim = Ped_Sldr_Marching01 PartialAnimOverlay = Ped_Sldr_Marching03_P
        change DrumPartialNumber = 3
        @ PlayAnimWithPartialAnimIfExists anim = Ped_Sldr_Marching01 PartialAnimOverlay = Ped_Sldr_Marching04_P
        change DrumPartialNumber = 4
        @ PlayAnimWithPartialAnimIfExists anim = Ped_Sldr_Marching01 PartialAnimOverlay = Ped_Sldr_Marching06_P
        change DrumPartialNumber = 6
    )
    Obj_WaitAnimFinished
    repeat 
endscript


script BO_STATECONTROL_drummer_drumstanding 
    Ped_StopMoving
    PlayAnimWithPartialAnimIfExists anim = Ped_Sldr_Stop01
    Obj_WaitAnimFinished
    getrandomvalue name = RepeatCount a = 3 b = 6 resolution = 3 integer
    begin 
    Random (
        @ PlayAnimWithPartialAnimIfExists anim = Ped_Sldr_Drumming02
        change DrumPartialNumber = 2
        @ PlayAnimWithPartialAnimIfExists anim = Ped_Sldr_Drumming02 PartialAnimOverlay = Ped_Sldr_Drumming04_P
        change DrumPartialNumber = 4
        @ PlayAnimWithPartialAnimIfExists anim = Ped_Sldr_Drumming02 PartialAnimOverlay = Ped_Sldr_Drumming06_P
        change DrumPartialNumber = 6
        @ PlayAnimWithPartialAnimIfExists anim = Ped_Sldr_Drumming02 PartialAnimOverlay = Ped_Sldr_Drumming08_P
        change DrumPartialNumber = 8
        @ PlayAnimWithPartialAnimIfExists anim = Ped_Sldr_Drumming02 PartialAnimOverlay = Ped_Sldr_Drumming10_P
        change DrumPartialNumber = 10
    )
    Obj_WaitAnimFinished
    repeat <RepeatCount>
    Ped_StartMoving
    PlayAnimWithPartialAnimIfExists anim = Ped_Sldr_Start
    Obj_WaitAnimFinished
    Ped_SetLogicState BO_STATE_drummer_drumwalking table = BO_StateLogic_Table
endscript


script BO_STATECONTROL_rollerblading 
    begin 
    Random (
        @*3 PlayAnimWithPartialAnimIfExists anim = Ped_Rollerblader_SkateCycle
        @ PlayAnimWithPartialAnimIfExists anim = Ped_Rollerblader_HeelClick
        @ PlayAnimWithPartialAnimIfExists anim = Ped_RollerBlader_ParadingAss
        @ PlayAnimWithPartialAnimIfExists anim = Ped_Rollerblader_StartHipPrance
        Obj_WaitAnimFinished
        getrandomvalue name = RepeatCount a = 1 b = 3 resolution = 3 integer
        begin 
        PlayAnimWithPartialAnimIfExists anim = Ped_Rollerblader_HipPrance
        Obj_WaitAnimFinished
        repeat <RepeatCount>
        PlayAnimWithPartialAnimIfExists anim = Ped_Rollerblader_StopHipPrance
        @ PlayAnimWithPartialAnimIfExists anim = Ped_RollerBlader_Spin2Back
        Obj_WaitAnimFinished
        getrandomvalue name = RepeatCount a = 1 b = 3 resolution = 3 integer
        begin 
        PlayAnimWithPartialAnimIfExists anim = Ped_Rollerblader_BackSkate
        Obj_WaitAnimFinished
        repeat <RepeatCount>
        PlayAnimWithPartialAnimIfExists anim = Ped_Rollerblader_Spin2Front
    )
    Obj_WaitAnimFinished
    repeat 
endscript


