script BO_startup 
    LoadAsset 'levels\BO\CAM_ComboBench.ska' desc = CAM_ComboBench
    LoadAsset 'levels\BO\CAM_SkateStoppers.ska' desc = CAM_SkateStoppers
    LoadAsset 'levels\BO\CAM_Tag.ska' desc = CAM_Tag
    if NOT InNetGame
        LoadAsset 'cutscenes\BO_SmallEvent_Scaffold.cut' desc = BO_SmallEvent_Scaffold
    endif
    LoadAsset 'cutscenes\BO_SmallEvent_Ship.cut' desc = BO_SmallEvent_Ship
    LoadAsset 'models\gameobjects\projectiles\apple\apple.tex'
    LoadAsset 'models\gameobjects\projectiles\apple\apple.mdl' parent = `models\gameobjects\projectiles\apple\apple.tex`
    if GameModeEquals is_career
        LoadAsset 'models\veh\veh_gurney\veh_gurney.tex'
        LoadAsset 'models\veh\veh_gurney\veh_gurney.skin' parent = `models\veh\veh_gurney\veh_gurney.tex`
    endif
    if InNetGame
        LoadAsset 'models\animals\anl_pigeon\anl_pigeon.tex'
        LoadAsset 'models\animals\anl_pigeon\anl_pigeon.skin' parent = `models/animals/anl_pigeon/anl_pigeon.tex`
        animload_anl_pigeon
    endif
    LoadSkeleton name = 'skeletons/CS_ShipEvent_Rope_R.ske' CS_ShipEvent_Rope_R
    LoadSkeleton name = 'skeletons/CS_ShipEvent_Rope_L.ske' CS_ShipEvent_Rope_L
    LoadSkeleton name = 'skeletons/CS_ShipEvent_Rope_F.ske' CS_ShipEvent_Rope_F
    LoadSkeleton name = 'skeletons/CS_ShipEvent_Rope_B.ske' CS_ShipEvent_Rope_B
endscript


