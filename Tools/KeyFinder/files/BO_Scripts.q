endscript


script BO_STATECONTROL_Paulie_GetAttention 
    printf 'Paulie Attention'
    get_current_skater_name
    if (<name> = segwayrider)
        printf 'Race Paulie Biotch!'
        setup_ped_speech {ped_id = TRG_Race_Ped
            inner_radius = 7
            outer_radius = 8
            accept_text = 'Press \m5 to Race.'
            activation_script = BO_Race_Ped_Go
        }
    else
        printf 'Talk to Paulie'
        setup_ped_speech {ped_id = TRG_Race_Ped
            inner_radius = 7
            outer_radius = 8
            accept_text = 'Press \m5 to Talk.'
            activation_script = BO_Paulie_TalkShit
            dont_deactivate_goals
        }
    endif
    PlayAnimWithPartialAnimIfExists anim = Wheelchair_Attention
    Obj_WaitAnimFinished
    begin 
    if NOT obj_objectinattentionradius
        printf 'Paulie losing attention'
        Ped_Utils_SetMiscEventHandlers
        Ped_StateLogic_SetDefaultState
        return 
    endif
    Random (
        @ 
        PlayAnimWithPartialAnimIfExists anim = Wheelchair_Attention
        @*3 
        PlayAnimWithPartialAnimIfExists anim = Wheelchair_Idle
    )
    Obj_WaitAnimFinished
    repeat 
endscript


script BO_Paulie_TalkShit 
    printf 'PUALIE TALKS SOME SHIT!!'
    if ScreenElementExists id = ped_speech_dialog
        speech_box_exit anchor_id = ped_speech_dialog
    endif
    UnpauseSkaters
    Random (
        @ hint_text = 'Come back when you\'re riding something with batteries, bitch.'
        vo = Paulie_NoRace01
        @ hint_text = 'A skateboard is no match for a sweet ride like mine!'
        vo = Paulie_NoRace02
        @ hint_text = 'Come back when you\'re riding something with real power, slacker.'
        vo = Paulie_NoRace03
    )
    create_speech_box {
        anchor_id = ped_hint_dialog
        text = <hint_text>
        no_pad_choose
        no_pad_start
        bg_rgba = [ 100 100 100 128 ]
        text_rgba = [ 128 128 128 128 ]
        Pos = (320.0, 400.0)
        bg_x_scale = 1.400
        z_priority = 5
    }
    if GotParam vo
        TRG_Race_Ped ::obj_playstream <vo> vol = GlobalHintVOVol dropoff = GlobalHintVODropoff
    endif
    TRG_Race_Ped ::Obj_WaitAnimFinished
    TRG_Race_Ped ::PlayAnimWithPartialAnimIfExists anim = Wheelchair_Idle
    TRG_Race_Ped ::Obj_WaitAnimFinished
    TRG_Race_Ped ::PlayAnimWithPartialAnimIfExists anim = Wheelchair_Spin
    TRG_Race_Ped ::Obj_WaitAnimFinished
    if ObjectExists id = ped_hint_dialog
        DestroyScreenElement id = ped_hint_dialog
    endif
    TRG_Race_Ped ::Ped_SetLogicState BO_STATE_Paulie_Waiting table = BO_StateLogic_Table
    UnpauseSkaters
endscript


