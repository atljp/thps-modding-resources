script AU_2d_sign_element_create parent = menu3d_anchor
    if GetGlobalFlag flag = SCREEN_MODE_WIDE
        <Offset> = (-8.0, 0.0)
    else
        <Offset> = (0.0, 0.0)
    endif
    CreateScreenElement {
        type = SpriteElement
        parent = <parent>
        texture = AU_icon_1
        id = AU_turtle
        Scale = 3.500
        rgba = [ 128 128 128 100 ]
        Pos = ((220.0, 310.0) + <Offset>)
        rot_angle = 0
        just = [ center center ]
        z_priority = -51
    }
    flame_angle = -90
    CreateScreenElement {
        type = SpriteElement
        parent = <parent>
        texture = AU_icon_2
        id = AU_flame
        Scale = 2.200
        rgba = [ 124 45 23 128 ]
        Pos = ((76.0, 182.0) + <Offset>)
        rot_angle = <flame_angle>
        just = [ center bottom ]
        z_priority = -51
    }
    RunScriptOnScreenElement id = AU_turtle AU_turtle_anim
    RunScriptOnScreenElement id = AU_flame AU_flame_anim
endscript


script AU_flame_anim 
    begin 
    new_pt = Random (@ (150.0, 215.0) @ (76.0, 182.0) @ (115.0, 152.0) @ (198.0, 192.0))
    DoMorph time = 0 Scale = 2.200 Pos = <new_pt> alpha = 1
    begin 
    DoMorph time = 0 alpha = 1
    DoMorph time = 0.08000 alpha = Random (@ 0.4000 @ 0.5000 @ 0.8000) rot_angle = Random (@ -89 @ -91)
    repeat Random (@ 10 @ 15 @ 20)
    DoMorph time = 1.500 Scale = 2.500 Pos = (<new_pt> + (5.0, -15.0))alpha = 0 rot_angle = -90
    wait Random (@ 1 @ 2 @ 3) seconds
    repeat 
endscript


script AU_turtle_anim 
    begin 
    DoMorph time = 0 rot_angle = -3
