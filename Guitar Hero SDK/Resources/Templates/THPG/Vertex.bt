// Submeshcafe vertex (Generally used in X360 models)

typedef struct
{
    Vec3 position <bgcolor=CL_VBFLOAT>;
    ushort u <bgcolor=CL_VBFLOAT, comment="/ 65535.0">;
    ushort v <bgcolor=CL_VBFLOAT, comment="/ 65535.0">;

    uint packed_normals <bgcolor=CL_VBFLOAT>;
    uint packed_tangent_a <bgcolor=CL_VBFLOAT>;
    uint packed_tangent_b <bgcolor=CL_VBFLOAT>;

    QBKey bad_food <bgcolor=CL_VBFLOAT>;
} THPGVertex;