SCRIPT cutscene_subtitle_create 
	IF GetGlobalFlag flag = NO_DISPLAY_CUTSCENE_SUBTITLES 
		RETURN 
	ENDIF 
	IF NOT GotParam text 
		RETURN 
	ENDIF 
	IF GotParam duration 
		<duration> = ( <duration> * 1000 ) 
	ENDIF 
	<font> = newtrickfont 
	<scale> = 0.69999998808 
	IF French 
		<font> = newtrickfont 
		<scale> = 0.69999998808 
	ELSE 
		IF Spanish 
			<font> = dialog 
			<scale> = 0.89999997616 
		ELSE 
			IF German 
				<font> = newtrickfont 
				<scale> = 0.69999998808 
			ELSE 
				IF Italian 
					<font> = newtrickfont 
					<scale> = 0.69999998808 
				ENDIF 
			ENDIF 
		ENDIF 
	ENDIF 
	create_panel_block { 
		id = cutscene_subtitle 
		text = <text> 
		font_face = <font> 
		rgba = [ 128 128 128 128 ] 
		pos = PAIR(320.00000000000, 390.00000000000) 
		time = <duration> 
		dims = PAIR(800.00000000000, 0.00000000000) 
		just = [ center top ] 
		scale = <scale> 
		internal_just = [ center top ] 
		parent = root_window 
		z_priority = 15 
	} 
ENDSCRIPT

SCRIPT cutscene_subtitle_cleanup 
	IF ScreenElementExists id = cutscene_subtitle 
		DestroyScreenElement id = cutscene_subtitle 
	ENDIF 
ENDSCRIPT


