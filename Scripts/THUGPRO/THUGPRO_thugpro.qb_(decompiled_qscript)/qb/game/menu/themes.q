SCRIPT thugpro_change_theme_reset_begin 
	SetScreenElementProps_IfExists id = the_score_sprite texture = WHITE 
	SetScreenElementProps_IfExists id = the_special_bar_special texture = WHITE 
	SetScreenElementProps_IfExists id = the_special_bar_sprite texture = WHITE 
	SetScreenElementProps_IfExists id = the_boardstance_sprite texture = WHITE 
	SetScreenElementProps_IfExists id = generic_sponsor_logo texture = WHITE 
	SetScreenElementProps_IfExists id = the_balance_meter texture = WHITE 
	THUGPRO_SetSpecialBarUpdateState off 
	destroy_panel_stuff 
ENDSCRIPT

SCRIPT thugpro_change_theme_reset_end 
	get_skater_number 
	SetSpecialBarColors skater = <skater_number> 
	create_panel_stuff 
	hide_all_hud_items 
	THUGPRO_SetSpecialBarUpdateState on 
	IF ScreenElementExists id = color_selection_menu 
	ENDIF 
	IF ( ( ScreenElementExists id = array_menu ) | ( ScreenElementExists id = color_selection_menu ) ) 
		IF ScreenElementExists id = pause_gradient 
			DestroyScreenElement id = pause_gradient 
		ENDIF 
		pause_menu_gradient on right 
	ENDIF 
ENDSCRIPT

SCRIPT thugpro_change_theme_reset 
	reset_theme_keys 
	thugpro_change_theme { 
		theme = <theme> 
		dont_change_global 
		dont_save 
		custom_theme_reset = 1 
	} 
	SetRegKeyValue { 
		Key = "BASE_THEME_NAME" 
		Type = IntOrChecksum 
		value = <theme> 
		theme 
	} 
	themes_menu_edit_custom from_theme_menu 
ENDSCRIPT

SCRIPT thugpro_change_theme 
	IF NOT GotParam loading 
		thugpro_change_theme_reset_begin 
	ENDIF 
	set_theme_default 
	IF GotParam value_checksum 
		theme = <value_checksum> 
	ENDIF 
	SWITCH <theme> 
		CASE custom 
			set_theme_custom 
		CASE thugpro 
			set_theme_thugpro custom_theme_reset = <custom_theme_reset> 
		CASE thps4 
			set_theme_thps4 custom_theme_reset = <custom_theme_reset> 
		CASE thug2 
			set_theme_thug2 custom_theme_reset = <custom_theme_reset> 
		CASE thaw 
		CASE thaw_alt 
			set_theme_thaw custom_theme_reset = <custom_theme_reset> 
		CASE thug1 
		CASE thug1_ug 
			set_theme_thug1 custom_theme_reset = <custom_theme_reset> 
		CASE thug1_pa 
			set_theme_thug1_pa custom_theme_reset = <custom_theme_reset> 
		CASE thug1_de 
			set_theme_thug1_de custom_theme_reset = <custom_theme_reset> 
		CASE thug1_al 
			set_theme_thug1_al custom_theme_reset = <custom_theme_reset> 
		CASE thug1_fu 
			set_theme_thug1_fu custom_theme_reset = <custom_theme_reset> 
		CASE thug1_ns 
			set_theme_thug1_ns custom_theme_reset = <custom_theme_reset> 
		CASE thug1_SP1 
			set_theme_thug1_SP1 custom_theme_reset = <custom_theme_reset> 
		CASE thug1_SP2 
			set_theme_thug1_SP2 custom_theme_reset = <custom_theme_reset> 
		CASE thug1_SP3 
			set_theme_thug1_SP3 custom_theme_reset = <custom_theme_reset> 
		CASE thug1_SP4 
			set_theme_thug1_SP4 custom_theme_reset = <custom_theme_reset> 
		CASE thug1_SP5 
			set_theme_thug1_SP5 custom_theme_reset = <custom_theme_reset> 
		CASE thug1_SS1 
			set_theme_thug1_SS1 custom_theme_reset = <custom_theme_reset> 
		CASE thug1_SS2 
			set_theme_thug1_SS2 custom_theme_reset = <custom_theme_reset> 
		CASE thug1_SS3 
			set_theme_thug1_SS3 custom_theme_reset = <custom_theme_reset> 
		CASE thug1_SS4 
			set_theme_thug1_SS4 custom_theme_reset = <custom_theme_reset> 
		DEFAULT 
			set_theme_thugpro custom_theme_reset = <custom_theme_reset> 
	ENDSWITCH 
	IF NOT GotParam dont_change_global 
		change THUGPRO_CurrentThemeName = <theme> 
	ENDIF 
	IF NOT GotParam loading 
		thugpro_change_theme_reset_end 
	ENDIF 
	IF GotParam update_menu 
		create_predisplay_options_menu 
	ENDIF 
	IF GotParam refresh_menu 
		GetTags 
		swap_themes_menu last_selected_item = <id> 
	ENDIF 
	IF NOT GotParam dont_save 
		thugpro_save_key_value Key = "CurrentTheme" value = <theme> 
	ENDIF 
ENDSCRIPT

SCRIPT swap_themes_menu 
	IF GotParam reset_theme_select 
		<menu_title> = "Select base theme" 
		<pad_back_params> = { callback = custom_theme_selection_menu } 
	ELSE 
		<menu_title> = "Interface Themes" 
		<pad_back_params> = { callback = create_predisplay_options_menu } 
	ENDIF 
	thugpro_menu_create { 
		menu_name = themes_options 
		menu_title = <menu_title> 
		menu_style = MENU_STYLE_RIGHT 
		pad_back_script = generic_menu_pad_back 
		pad_back_params = <pad_back_params> 
		helper_text = generic_helper_text 
		num_items_to_show = 11 
		show_description_panel 
		scrolling 
		boxed 
	} 
	IF GotParam reset_theme_select 
		IF GotParam initialize_custom_theme 
			change THUGPRO_CurrentThemeName = custom 
			thugpro_save_key_value Key = "CurrentTheme" value = custom 
		ENDIF 
		<pad_choose_script> = thugpro_change_theme_reset 
	ELSE 
		<pad_choose_script> = thugpro_change_theme 
	ENDIF 
	IF NOT GotParam reset_theme_select 
		make_thugpro_menu_item { 
			text = "Custom Theme" 
			pad_choose_script = custom_theme_selection_menu 
			pad_choose_params = <...> 
		} 
	ENDIF 
	<themes_array> = [ 
		{ text = "THUG PRO" id = thugpro pad_choose_params = { refresh_menu theme = thugpro } } 
		{ text = "Cloudbuster" id = thug1_de pad_choose_params = { refresh_menu theme = thug1_de } } 
		{ text = "Destruction Tour" id = thug2 pad_choose_params = { refresh_menu theme = thug2 } } 
		{ text = "THPS4" id = thps4 pad_choose_params = { refresh_menu theme = thps4 } } 
		{ text = "Retro Hawk" id = thug1_pa pad_choose_params = { refresh_menu theme = thug1_pa } } 
		{ text = "American Wasteland" id = thaw pad_choose_params = { refresh_menu theme = thaw } } 
		{ text = "Chilled" id = thug1_al pad_choose_params = { refresh_menu theme = thug1_al } } 
		{ text = "Skullz \'n Stuff" id = thug1_fu pad_choose_params = { refresh_menu theme = thug1_fu } } 
		{ text = "Neversoft" id = thug1_ns pad_choose_params = { refresh_menu theme = thug1_ns } } 
		{ text = "T . H . U . G ." id = thug1 pad_choose_params = { refresh_menu theme = thug1 } } 
		{ text = "Birdhouse" id = thug1_SP1 pad_choose_params = { refresh_menu theme = thug1_SP1 } } 
		{ text = "Element" id = thug1_SP2 pad_choose_params = { refresh_menu theme = thug1_SP2 } } 
		{ text = "Flip" id = thug1_SP3 pad_choose_params = { refresh_menu theme = thug1_SP3 } } 
		{ text = "Girl" id = thug1_SP4 pad_choose_params = { refresh_menu theme = thug1_SP4 } } 
		{ text = "ZERO" id = thug1_SP5 pad_choose_params = { refresh_menu theme = thug1_SP5 } } 
		{ text = "KISS" id = thug1_SS1 pad_choose_params = { refresh_menu theme = thug1_SS1 } } 
		{ text = "Ironman" id = thug1_SS2 pad_choose_params = { refresh_menu theme = thug1_SS2 } } 
		{ text = "T.H.U.D" id = thug1_SS3 pad_choose_params = { refresh_menu theme = thug1_SS3 } } 
		{ text = "THUG1 (alt)" id = thug1_SS4 pad_choose_params = { refresh_menu theme = thug1_SS4 } } 
	] 
	ForEachIn <themes_array> do = make_thugpro_menu_item params = { pad_choose_script = <pad_choose_script> } 
	refresh_edit_theme_preveiw_specialmeter force_create 
	refresh_edit_theme_preveiw_balancemeter force_create 
	IF GotParam last_selected_item 
		focus_target = <last_selected_item> 
	ENDIF 
	thugpro_menu_finalize <...> 
ENDSCRIPT

SCRIPT custom_theme_selection_menu 
	IF RegKeyExist Key = "BASE_THEME_NAME" theme 
		thugpro_change_theme theme = custom 
	ELSE 
		<menu_edit_focusable> = not_focusable 
	ENDIF 
	refresh_edit_theme_preveiw_specialmeter force_create 
	refresh_edit_theme_preveiw_balancemeter force_create 
	<menu_title> = "Custom theme" 
	<pad_back_params> = { callback = swap_themes_menu } 
	thugpro_menu_create { 
		menu_name = custom_theme_options 
		menu_title = <menu_title> 
		menu_style = MENU_STYLE_RIGHT 
		pad_back_script = generic_menu_pad_back 
		pad_back_params = <pad_back_params> 
		helper_text = generic_helper_text 
		show_description_panel 
		boxed 
	} 
	make_thugpro_menu_item { 
		text = "Edit custom theme" 
		id = menu_edit_custom_theme 
		pad_choose_script = themes_menu_edit_custom 
		pad_choose_params = { from_theme_menu } 
		<menu_edit_focusable> 
	} 
	make_thugpro_menu_item { 
		text = "Reset custom theme" 
		id = menu_reset_custom_theme 
		pad_choose_script = swap_themes_menu 
		pad_choose_params = { reset_theme_select } 
	} 
	thugpro_menu_finalize <...> 
ENDSCRIPT

SCRIPT refresh_edit_theme_preveiw_specialmeter 
	IF GotParam force_create 
		destroy_edit_theme_preveiw_specialmeter 
		create_edit_theme_preveiw_specialmeter 
	ELSE 
		IF ScreenElementExists id = edit_theme_preview_container_special 
			destroy_edit_theme_preveiw_specialmeter 
			create_edit_theme_preveiw_specialmeter 
		ENDIF 
	ENDIF 
ENDSCRIPT

SCRIPT destroy_edit_theme_preveiw_specialmeter 
	IF ScreenElementExists id = edit_theme_preview_container_special 
		DestroyScreenElement id = edit_theme_preview_container_special 
	ENDIF 
ENDSCRIPT

