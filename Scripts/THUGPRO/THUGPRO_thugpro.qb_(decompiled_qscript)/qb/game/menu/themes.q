SCRIPT do_theme_preveiw_special_meter_pulse 
	min_scale = ( PAIR(0.00000000000, 1.00000000000) * ( ( ( THUGPRO_GlobalThemeInfo ) . SPECIAL_BAR_SPRITE_SCALE ) . PAIR(0.00000000000, 1.00000000000) ) ) 
	BEGIN 
		DoMorph scale = ( ( THUGPRO_GlobalThemeInfo ) . SPECIAL_BAR_SPRITE_SCALE ) time = 0.40000000596 
		DoMorph rgba = ( ( ( THUGPRO_GlobalThemeInfo ) . SPECIAL_BAR_COLORS ) [ 0 ] ) time = 0.30000001192 
		wait 0.30000001192 seconds 
		DoMorph rgba = ( ( ( THUGPRO_GlobalThemeInfo ) . SPECIAL_BAR_COLORS ) [ 1 ] ) time = 0.30000001192 
		wait 0.30000001192 seconds 
		DoMorph rgba = ( ( ( THUGPRO_GlobalThemeInfo ) . SPECIAL_BAR_COLORS ) [ 2 ] ) time = 0.30000001192 
		wait 0.30000001192 seconds 
		DoMorph rgba = ( ( ( THUGPRO_GlobalThemeInfo ) . SPECIAL_BAR_COLORS ) [ 0 ] ) time = 0.30000001192 
		wait 0.30000001192 seconds 
		DoMorph rgba = ( ( ( THUGPRO_GlobalThemeInfo ) . SPECIAL_BAR_COLORS ) [ 1 ] ) time = 0.30000001192 
		wait 0.30000001192 seconds 
		DoMorph rgba = ( ( ( THUGPRO_GlobalThemeInfo ) . SPECIAL_BAR_COLORS ) [ 2 ] ) time = 0.30000001192 
		wait 0.30000001192 seconds 
		DoMorph scale = <min_scale> time = 0.40000000596 
		wait 0.40000000596 seconds 
	REPEAT 
ENDSCRIPT

