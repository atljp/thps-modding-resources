	ENDIF 
ENDSCRIPT

SCRIPT thugpro_maybe_apply_theme_setting 
	IF GotParam key_name 
		IF RegKeyExist Key = <key_name> theme 
			IF GetRegKeyValue Key = <key_name> Type = IntOrChecksum theme 
				IF GotParam color_options 
					IF NOT GotParam load_func 
						<load_func> = edit_theme_set_color_value 
					ENDIF 
					FormatText ChecksumName = key_name_checksum "%a" a = <key_name> 
					local_load_func_params = { 
						key_name_checksum = <key_name_checksum> 
						key_name = <key_name> 
						rgba_index = <value_int> 
						dont_save 
						key_name = <key_name> 
						value = <value_checksum> 
						value_int = <value_int> 
						value_checksum = <value_checksum> 
						loading 
					} 
					<load_func> <local_load_func_params> 
				ELSE 
					IF GotParam load_func 
						local_load_func_params = { 
							<load_func_params> 
							key_name = <key_name> 
							value = <value_checksum> 
							value_int = <value_int> 
							value_checksum = <value_checksum> 
							loading 
						} 
						<load_func> <local_load_func_params> 
					ENDIF 
				ENDIF 
			ENDIF 
		ENDIF 
	ENDIF 
