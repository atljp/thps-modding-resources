SCRIPT edit_theme_set_special_meter_sponsor_icon key_name = "SPONSOR_TEXTURE" 
	printf "edit_theme_set_special_meter_sponsor_icon %a" a = <key_name> 
	IF NOT GotParam loading 
		thugpro_change_theme_reset_begin 
	ENDIF 
	IF GotParam value_checksum 
		value = <value_checksum> 
	ENDIF 
	printf "value %v" v = <value> 
	SWITCH <value> 
		CASE none 
			set_theme_no_sponsor_icon 
			change g_edit_theme_set_special_meter_sponsor_icon_index = 0 
		CASE thug2 
			set_theme_thug2_sponsor_icon 
			change g_edit_theme_set_special_meter_sponsor_icon_index = 1 
		CASE thaw 
			set_theme_thaw_sponsor_icon 
			change g_edit_theme_set_special_meter_sponsor_icon_index = 2 
		CASE thug1_de 
			set_theme_thug1_de_sponsor_icon 
			change g_edit_theme_set_special_meter_sponsor_icon_index = 4 
		CASE thug1_al 
			set_theme_thug1_al_sponsor_icon 
			change g_edit_theme_set_special_meter_sponsor_icon_index = 5 
		CASE thug1_fu 
			set_theme_thug1_fu_sponsor_icon 
			change g_edit_theme_set_special_meter_sponsor_icon_index = 6 
		CASE thug1_ns 
			set_theme_thug1_ns_sponsor_icon 
			change g_edit_theme_set_special_meter_sponsor_icon_index = 7 
		CASE thug1 
		CASE thug1_ug 
			set_theme_thug1_ug_sponsor_icon 
			change g_edit_theme_set_special_meter_sponsor_icon_index = 8 
