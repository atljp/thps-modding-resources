SCRIPT thugpro_maybe_read_theme_reg_key 
	IF GotParam key_name 
		IF GotParam force_reset 
			IF GotParam options_index_global_default 
				change globalname = <options_index_global> newvalue = <options_index_global_default> 
			ENDIF 
			IF GotParam default_value 
				SetRegKeyValue { 
					Key = <key_name> 
					Type = IntOrChecksum 
					value = <default_value> 
					theme 
				} 
			ENDIF 
		ELSE 
			IF RegKeyExist Key = <key_name> theme 
				IF GetRegKeyValue Key = <key_name> Type = IntOrChecksum theme 
				ENDIF 
			ELSE 
				IF GotParam options_index_global_default 
					change globalname = <options_index_global> newvalue = <options_index_global_default> 
				ENDIF 
				IF GotParam default_value 
					SetRegKeyValue { 
						Key = <key_name> 
						Type = IntOrChecksum 
						value = <default_value> 
						theme 
					} 
				ENDIF 
			ENDIF 
		ENDIF 
	ENDIF 
ENDSCRIPT

