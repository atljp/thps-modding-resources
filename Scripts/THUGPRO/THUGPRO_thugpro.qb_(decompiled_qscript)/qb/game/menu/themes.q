SCRIPT create_edit_theme_color_menu 
	change edit_theme_menu_last_selected_item = <container_id> 
	thugpro_menu_create { 
		menu_name = color_selection 
		menu_title = "Color selection" 
		menu_style = MENU_STYLE_RIGHT 
		pad_back_script = themes_menu_edit_custom 
		helper_text = generic_helper_text 
		scrolling 
		boxed 
	} 
	FormatText ChecksumName = key_name_checksum "%a" a = <key_name> 
	GetArraySize THUGPRO_THEME_COLOR_ARRAY 
	index = 0 
	BEGIN 
		FormatText ChecksumName = menu_item_id "menu_color_item_%r_%g_%b_%a" r = ( ( THUGPRO_THEME_COLOR_ARRAY [ <index> ] ) [ 0 ] ) g = ( ( THUGPRO_THEME_COLOR_ARRAY [ <index> ] ) [ 1 ] ) b = ( ( THUGPRO_THEME_COLOR_ARRAY [ <index> ] ) [ 2 ] ) a = ( ( THUGPRO_THEME_COLOR_ARRAY [ <index> ] ) [ 3 ] ) 
		edit_theme_add_color_item { 
			id = <menu_item_id> 
			rgba = ( THUGPRO_THEME_COLOR_ARRAY [ <index> ] ) 
			load_func = <load_func> 
			pad_choose_params = { 
				key_name_checksum = <key_name_checksum> 
				key_name = <key_name> 
				rgba_index = <index> 
			} 
			focus_params = { 
				preview_item_id = <preview_item_id> 
				preview_rgba = ( THUGPRO_THEME_COLOR_ARRAY [ <index> ] ) 
			} 
		} 
		<index> = ( <index> + 1 ) 
	REPEAT <array_size> 
	FormatText ChecksumName = last_selected_item "menu_color_item_%r_%g_%b_%a" r = ( ( <preview_rgba> ) [ 0 ] ) g = ( ( <preview_rgba> ) [ 1 ] ) b = ( ( <preview_rgba> ) [ 2 ] ) a = ( ( <preview_rgba> ) [ 3 ] ) 
	thugpro_menu_finalize <...> focus_target = <last_selected_item> 
ENDSCRIPT

