SCRIPT create_edit_theme_array_menu 
	FormatText ChecksumName = container_id "%s" s = <display_name> 
	MangleChecksums a = create_edit_theme_array_menu b = <container_id> 
	change edit_theme_menu_last_selected_item = <mangled_id> 
	thugpro_menu_create { 
		menu_name = array 
		menu_title = <display_name> 
		menu_style = MENU_STYLE_RIGHT 
		pad_back_script = themes_menu_edit_custom 
		helper_text = generic_helper_text 
		scrolling 
		boxed 
	} 
	IF GotParam option_pad_choose 
		pad_choose_script = <option_pad_choose> 
	ENDIF 
	ForEachIn <option_array> do = add_edit_theme_menu_item params = { pad_choose_script = <pad_choose_script> key_name = <key_name> } 
	thugpro_menu_create_remainder_items 
	thugpro_menu_finalize <...> 
ENDSCRIPT

SCRIPT add_edit_theme_menu_item 
	IF NOT GotParam NO_MENU_ITEM 
		IF NOT GotParam pad_choose_script 
			pad_choose_script = nullscript 
		ENDIF 
		IF GotParam option_array 
			IF GotParam options_index_global 
				extra_text = ( ( <option_array> [ ( <options_index_global> ) ] ) . display_name ) 
			ELSE 
				extra_text = "\\c2TEST" 
			ENDIF 
			make_thugpro_menu_item text = <display_name> extra_text = <extra_text> pad_choose_script = create_edit_theme_array_menu pad_choose_params = <...> 
		ELSE 
			IF GotParam color_options 
				IF NOT GotParam load_func 
					load_func = edit_theme_set_color_value 
				ENDIF 
				IF ( <key_name> = "SPECIAL_BAR_COLOR_A" ) 
					preview_rgba = ( ( ( THUGPRO_GlobalThemeInfo ) . SPECIAL_BAR_COLORS ) [ 0 ] ) 
				ENDIF 
				IF ( <key_name> = "SPECIAL_BAR_COLOR_B" ) 
					preview_rgba = ( ( ( THUGPRO_GlobalThemeInfo ) . SPECIAL_BAR_COLORS ) [ 1 ] ) 
				ENDIF 
				IF ( <key_name> = "SPECIAL_BAR_COLOR_C" ) 
					preview_rgba = ( ( ( THUGPRO_GlobalThemeInfo ) . SPECIAL_BAR_COLORS ) [ 2 ] ) 
				ENDIF 
				FormatText ChecksumName = key_name_rgba "%a" a = <key_name> 
				preview_rgba = ( ( THUGPRO_GlobalThemeInfo ) . ( <key_name_rgba> ) ) 
				FormatText ChecksumName = container_id "%a" a = <display_name> 
				make_thugpro_menu_item { 
					id = <container_id> 
					text = <display_name> 
					pad_choose_script = create_edit_theme_color_menu 
					pad_choose_params = <...> 
					child_texture_persistent 
					child_texture = white2 
					child_texture_rgba = <preview_rgba> 
					child_texture_offset = PAIR(76.00000000000, 10.00000000000) 
					child_texture_scale = PAIR(4.50000000000, 2.20000004768) 
					child_texture_alpha = 1.00000000000 
				} 
			ELSE 
				make_thugpro_menu_item text = <display_name> pad_choose_script = <pad_choose_script> pad_choose_params = <...> 
			ENDIF 
		ENDIF 
	ENDIF 
ENDSCRIPT

