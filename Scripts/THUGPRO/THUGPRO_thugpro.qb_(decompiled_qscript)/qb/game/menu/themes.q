		} 
		edit_theme_set_special_meter_type { 
			value = thps4 
			loading 
		} 
		edit_theme_set_special_meter_sponsor_icon { 
			value = none 
			loading 
		} 
	ELSE 
		set_theme_thps4_balance_meter_sprites 
		set_theme_thps4_special_meter_sprites 
		set_theme_no_sponsor_icon 
	ENDIF 
