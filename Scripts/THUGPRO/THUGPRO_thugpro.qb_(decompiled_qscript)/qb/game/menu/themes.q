SCRIPT edit_theme_set_special_meter_bar_colors 
	printf "edit_theme_set_special_meter_bar_colors" 
	IF NOT GotParam loading 
		thugpro_change_theme_reset_begin 
	ENDIF 
	IF NOT ( <rgba_index> = -1 ) 
		IF ( <key_name> = "SPECIAL_BAR_COLOR_A" ) 
			THUGPRO_SetThemeValue { 
				name = SPECIAL_BAR_COLORS 
				Type = array 
				value = ( THUGPRO_THEME_COLOR_ARRAY [ <rgba_index> ] ) 
				array_index = 0 
			} 
			IF NOT GotParam dont_save 
				SetRegKeyValue { 
					Key = "SPECIAL_BAR_COLOR_A" 
					Type = IntOrChecksum 
					value = <rgba_index> 
					theme 
				} 
			ENDIF 
		ENDIF 
		IF ( <key_name> = "SPECIAL_BAR_COLOR_B" ) 
			THUGPRO_SetThemeValue { 
				name = SPECIAL_BAR_COLORS 
				Type = array 
				value = ( THUGPRO_THEME_COLOR_ARRAY [ <rgba_index> ] ) 
				array_index = 1 
			} 
			IF NOT GotParam dont_save 
				SetRegKeyValue { 
					Key = "SPECIAL_BAR_COLOR_B" 
					Type = IntOrChecksum 
					value = <rgba_index> 
					theme 
				} 
			ENDIF 
		ENDIF 
		IF ( <key_name> = "SPECIAL_BAR_COLOR_C" ) 
			THUGPRO_SetThemeValue { 
				name = SPECIAL_BAR_COLORS 
				Type = array 
				value = ( THUGPRO_THEME_COLOR_ARRAY [ <rgba_index> ] ) 
				array_index = 2 
			} 
			IF NOT GotParam dont_save 
				SetRegKeyValue { 
					Key = "SPECIAL_BAR_COLOR_C" 
					Type = IntOrChecksum 
					value = <rgba_index> 
					theme 
				} 
			ENDIF 
		ENDIF 
	ENDIF 
	IF NOT GotParam loading 
		refresh_edit_theme_preveiw_specialmeter 
		refresh_edit_theme_preveiw_balancemeter 
		thugpro_change_theme_reset_end 
	ENDIF 
ENDSCRIPT

